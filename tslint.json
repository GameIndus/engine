{
    "defaultSeverity": "warning",
    "extends": "tslint:recommended",
    "jsRules": {},
    "rules": {
        "no-console": false,
        "variable-name": [
            true,
            "ban-keywords",
            "check-format",
            "allow-leading-underscore"
        ],
        "interface-name": [
            true,
            "never-prefix"
        ],
        "no-namespace": false
    },
    "rulesDirectory": []
}
